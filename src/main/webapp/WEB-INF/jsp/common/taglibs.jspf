<%@ page contentType="text/html;charset=UTF-8" language="java"%>
<%@ page import="com.tmwsoft.common.SystemCache"%>
<%@ page import="com.tmwsoft.common.DictConstants"%>
<%@ taglib prefix="stripes" uri="http://stripes.sourceforge.net/stripes.tld" %>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt" %>
<%@ taglib prefix="fn" uri="http://java.sun.com/jsp/jstl/functions" %>
<c:set var="root" value="${pageContext.request.contextPath}"/>

<c:set var="users" value="<%=SystemCache.getUsers()%>" scope="page"/>
<c:set var="dicts" value="<%=SystemCache.getDicts()%>" scope="page"/>
<c:set var="extDicts" value="<%=SystemCache.getExtDicts()%>" scope="page"/>
<c:set var="dictcodes" value="<%=DictConstants.getDictCodes()%>" scope="page"/>
